<script>
(function () {
  // get data as a JSON encoded string
  var message = {
    name: <%= name.to_json.html_safe %>,
    data: <%= data.to_json.html_safe %>
  };

  var origins = ['http://localhost:4200', 'https://noutu.be'];

  // send to opener
  if (window.opener) {
    origins.forEach((origin) => {
      window.opener.postMessage(message, origin);
    });
  }
})();
</script>
